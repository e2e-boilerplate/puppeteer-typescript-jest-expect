{
  "name": "puppeteer-es6modules-typescript-jest",
  "version": "1.0.0",
  "description": "Puppeteer end-to-end test automation boilerplate project. Using ES6 modules, TypeScript, and jest",
  "scripts": {
    "pretty": "npx prettier --no-config './__tests__/*.ts' --write",
    "lint": "npx tslint -c tslint.json './__tests__/*.ts'",
    "test": "npx jest"
  },
  "keywords": [
    "puppeteer",
    "es6 modules",
    "typescript",
    "jest",
    "boilerplate",
    "example"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "@types/is-ci": "^2.0.0",
    "@types/jest": "24.0.24",
    "@types/node": "12.12.24",
    "@types/puppeteer": "2.0.0",
    "husky": "3.1.0",
    "is-ci": "^2.0.0",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "puppeteer": "2.0.0",
    "ts-jest": "24.3.0",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
